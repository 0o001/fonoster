{
  "name": "root",
  "private": true,
  "devDependencies": {
    "lerna": "^3.22.0",
    "typescript": "^3.9.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier-standard": "^9.1.1"
  },
  "lint-staged": {
