{
  "name": "@fonoster/apiserver",
  "version": "0.4.5",
  "description": "Fonoster API Server",
  "author": "Pedro Sanders <psanders@fonoster.com>",
  "homepage": "https://github.com/fonoster/fonoster#readme",
  "license": "MIT",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "prebuild": "rimraf ./dist tsconfig.tsbuildinfo",
    "build": "tsc -b tsconfig.json"
  },
  "nodemonConfig": {
    "watch": [
      "../"
    ],
    "ext": "ts",
    "exec": "ts-node",
    "ignore": "**/*.d.ts"
  },
  "directories": {
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fonoster/fonoster.git"
  },
  "bugs": {
    "url": "https://github.com/fonoster/fonoster/issues"
  },
  "dependencies": {
    "@fonoster/agents": "^0.4.5",
    "@fonoster/apps": "^0.4.5",
    "@fonoster/auth": "^0.4.5",
    "@fonoster/callmanager": "^0.4.5",
    "@fonoster/common": "^0.4.5",
    "@fonoster/domains": "^0.4.5",
    "@fonoster/funcs": "^0.4.5",
    "@fonoster/limiter": "^0.4.5",
    "@fonoster/monitor": "^0.4.5",
    "@fonoster/numbers": "^0.4.5",
    "@fonoster/projects": "^0.4.5",
    "@fonoster/providers": "^0.4.5",
    "@fonoster/secrets": "^0.4.5",
    "@fonoster/storage": "^0.4.5",
    "@fonoster/users": "^0.4.5",
    "dotenv": "^8.2.0"
  },
  "gitHead": "9103932a97973f9bb4ceef7fd58119816a75646e"
}
