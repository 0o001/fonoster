{
  "name": "@yaps/ctl",
  "description": "Command-Line for YAPS",
  "version": "0.0.3",
  "author": "Pedro Sanders @psanders",
  "bin": {
    "yaps": "./bin/run"
  },
  "bugs": "https://github.com/fonoster/yaps/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@yaps/appmanager": "^0.0.3",
    "@yaps/core": "^0.0.3",
    "@yaps/providers": "^0.0.3",
    "@yaps/domains": "^0.0.3",
    "@yaps/agents": "^0.0.3",
    "@yaps/numbers": "^0.0.3",
    "cli-ux": "^5.4.5",
    "dotenv": "^8.2.0",
    "easy-table": "^1.1.1",
    "inquirer": "^7.1.0",
    "moment": "^2.24.0",
    "node-plop": "^0.25.0",
    "ora": "^4.0.3",
    "prettyjson": "^1.2.1",
    "truncate": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/fonoster/yaps",
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "yaps",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "apps": {
        "description": "manage voice applications"
      },
      "domains": {
        "description": "manage sip domains"
      },
      "agents": {
        "description": "manage sip agents"
      },
      "numbers": {
        "description": "manage sip numbers"
      },
      "providers": {
        "description": "manage sip providers"
      },
      "login": {
        "description": "authenticates current station"
      },
      "logout": {
        "description": "revoke crendentials to the current station"
      }
    }
  },
  "repository": "fonoster/yaps",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
